<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>signinserv</servlet-name>
        <servlet-class>com.netcracker.edu.airagency.SignInOrRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signinserv</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>sand</servlet-name>
        <servlet-class>com.netcracker.edu.airagency.sandbox.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sand</servlet-name>
        <url-pattern>/sanduri</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>buy</servlet-name>
        <servlet-class>com.netcracker.edu.airagency.BuyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>buy</servlet-name>
        <url-pattern>/buy</url-pattern>
    </servlet-mapping>
    <!--   <servlet>
           <servlet-name>thirdJSP</servlet-name>
           <jsp-file>third.jsp</jsp-file>
       </servlet>
       <servlet-mapping>
           <servlet-name>thirdJSP</servlet-name>
           <url-pattern>/third</url-pattern>
       </servlet-mapping>-->
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <session-config>
        <session-timeout>01</session-timeout>
    </session-config>
    <listener>
        <description>sessionListener</description>
        <listener-class>com.netcracker.edu.airagency.Listener</listener-class>
    </listener>
</web-app>
